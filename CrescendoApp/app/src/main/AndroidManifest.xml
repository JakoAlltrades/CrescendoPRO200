<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="crescendo.com.crescendoapp" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sign_Up" android:label="@string/app_name" ></activity>
        <activity android:name=".Chords" android:label="@string/app_name" ></activity>
        <activity android:name=".Metronome" android:label="@string/app_name" ></activity>
        <activity android:name=".Record" android:label="@string/app_name" ></activity>
        <activity android:name=".Scales" android:label="@string/app_name" ></activity>
        <activity android:name=".viewtune" android:label="@string/app_name" ></activity>
        <activity android:name=".Tune" android:label="@string/app_name" ></activity>
        <activity android:name=".UserData" android:label="@string/app_name" ></activity>
        <activity android:name=".UserHome" android:label="@string/app_name" ></activity>
        <activity android:name=".ViewRecordings" android:label="@string/app_name" ></activity>
        <activity android:name=".Tuner" android:label="@string/app_name" ></activity>
        <activity android:name="github.bewantbe.audio_analyzer_for_android.AnalyzerActivity" android:label="@string/app_name" ></activity>
    </application>

</manifest>