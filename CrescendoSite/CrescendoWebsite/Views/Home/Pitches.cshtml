@using CrescendoWebsite.Models

@model List<Pitch>

@{
    ViewBag.title = "Recordings";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Recordings</title>
</head>
<body>
    <audio src=~/DataHolding/Satisfaction.mp3 controls />
    <h1>From here you can view all of your recordings</h1>
    <div>
        @foreach (Pitch r in Model)
        {
            <h1>r.PitchName;</h1>
        }
    </div>
</body>
</html>

